<div class="row">
        <div class="col s6">
        <form action='/coindetail' method='POST'>
            <div class="input-field" id="searchbaronmain">
                <input id="search" class="z-depth-3" type="search" name='search' placeholder='Search by Name or TickerID' required>
                <label class="label-icon" for="search"></label>
                <i class="material-icons magnifying white-text text-darken-2">search</i>
            </div>
        </div>
        <div class='col s6'>
            <button class="btn waves-effect waves-light z-depth-3" type="submit" name="action">Submit</button>
        </div>
        </form>  
    </div> 

<h1>Searc Results</h1>
</hr>




    <div class="row">
            <div class="col s12 m6">
              <div class="card">
                <div class="card-image">
                  <img src="https://www.cryptocompare.com<%= coinDetail.ImageUrl %>">
                  <span class="card-title"><%= coinDetail.CoinName %></span>
                  <a class="btn-floating halfway-fab waves-effect waves-light red"><i class="material-icons">add</i></a>
                </div>
                <div class="card-content">
                        <p><span>Name: </span> <%= coinDetail.FullName %></p>
                        <p><span>Algorithm: </span> <%= coinDetail.Algorithm %></p>
                        <p><span>TotalCoinSupply: </span> <%= coinDetail.TotalCoinSupply %></p>
                        <p><span>TotalCoinsMined: </span> <%= coinDetail.TotalCoinsMined %></p>
                </div>
              </div>
            </div>
          </div>
