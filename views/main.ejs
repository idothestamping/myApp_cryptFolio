<div class="se-pre-con"></div>

<h2>Main Page Logged IN</h2>
<div class="row">
        <div class="col s12 m6 left">
            <form action='#' method='GET'>
            
                    <div class="input-field">
                        <input id="search" class="z-depth-3" type="search" name='search' placeholder='Search by Name or Ticker' required>
                        <label class="label-icon" for="search"></label>
                        <i class="material-icons magnifying white-text text-darken-2">search</i>
            </form>  
        </div>
        <div class='col s12 m6 left'>
                <form action="/coindetail" method='POST'>   
                <select name='selectionbox' class="browser-default selectionbox black-text z-depth-3">
                <option value="null" selected>Ticker/Coin Name</option>
                <% for(i = 0; i < coin.length; i++ ) {%>
                  <option value="<%= coin[i].code %>"><%= coin[i].name %>/<%= coin[i].code %></option>
                    <% } %>

                </select>
      
        </div>

<div class='col s12 m6 left'>
       
                <button class="btn btn-large waves-effect waves-light z-depth-3" type="submit" name="action">Select
                <!-- <i class="material-icons">Search</i> -->
                </button>
                </div>
            </form>
</div>
        

</div>
    